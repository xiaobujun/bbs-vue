{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n// @ is an alias to /src\nimport Header from \"@/components/Header\";\nexport default {\n  name: 'Index',\n  components: {\n    Header: Header\n  },\n  data: function data() {\n    return {\n      articleCard: [{\n        articleImage: \"\",\n        title: '这只是一个测试'\n      }]\n    };\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuDA;AAEA;AAEA;EAEAA,aAFA;EAGAC;IACAC;EADA,CAHA;EAMAC,IANA,kBAMA;IACA;MACAC,cACA;QACAC,gBADA;QAEAC;MAFA,CADA;IADA;EAQA;AAfA","names":["name","components","Header","data","articleCard","articleImage","title"],"sourceRoot":"src/views","sources":["IndexView.vue"],"sourcesContent":["<template>\n  <div>\n    <!--导航栏-->\n    <Header></Header>\n    <!--内容主体区-->\n    <main>\n      <div class=\"main_container\">\n        <!--轮播图-->\n        <div class=\"banner_container\">\n          <el-carousel :interval=\"4000\" type=\"card\" height=\"200px\">\n            <el-carousel-item v-for=\"item in 6\" :key=\"item\">\n              <h3 class=\"medium\">{{ item }}</h3>\n            </el-carousel-item>\n          </el-carousel>\n        </div>\n        <!--文章列表-->\n        <article>\n          <div class=\"article_container\">\n            <el-card class=\"box-card\">\n              <router-link to='/'>\n                <i>\n                  <el-image :src=\"articleCard.articleImage\">\n                  </el-image>\n                </i>\n                <div>\n                  <h2>{{articleCard.title}}</h2>\n                  <p></p>\n                  <span>{{}}</span>\n                  <span>{{}}</span>\n                  <span>{{}}</span>\n                </div>\n\n              </router-link>\n            </el-card>\n          </div>\n          <el-pagination class=\"mpage\"\n                         background\n                         layout=\"prev, pager, next\"\n                         :current-page=\"currentPage\"\n                         :page-size=\"pageSize\"\n                         :total=\"total\"\n                         @current-change=page>\n          </el-pagination>\n\n        </article>\n        <aside>\n          <!--官方公告-->\n          <div></div>\n        </aside>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport Header from \"@/components/Header\";\n\nexport default {\n\n  name: 'Index',\n  components: {\n    Header\n  },\n  data() {\n    return {\n      articleCard:[\n        {\n          articleImage:\"\",\n          title:'这只是一个测试',\n        }\n      ]\n    }\n  }\n}\n</script>\n<style>\n.banner_container {\n  width: 100%;\n  margin: 0 auto;\n}\n\n.main_container {\n  width: 70%;\n  margin: 2% auto;\n}\n\n.el-carousel__item h3 {\n  color: #475669;\n  font-size: 14px;\n  opacity: 0.75;\n  line-height: 200px;\n  margin: 0;\n}\n\n.el-carousel__item:nth-child(2n) {\n  background-color: #99a9bf;\n}\n\n.el-carousel__item:nth-child(2n+1) {\n  background-color: #d3dce6;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}